                                                        \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END EXP MACROS\n\n\n// Robot configuration code.\ninertial BrainInertial = inertial();\n\n\n\n// Generated code.\n\n// Include the EXP Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nint Brain_precision = 0, Console_precision = 0;\n\nfloat myVariable, Row, Col, Driving_Lane;\n\nfloat Park_It[5][5];\n\n// \"when started\" hat block\nint whenStarted1() {\n  Col = 1.0;\n  Row = 1.0;\n  Park_It[0][0] = 0.0;\n  Park_It[0][1] = 0.0;\n  Park_It[0][2] = 0.0;\n  Park_It[0][3] = 0.0;\n  Park_It[0][4] = 0.0;\n  Park_It[1][0] = 0.0;\n  Park_It[1][1] = 2.0;\n  Park_It[1][2] = 1.0;\n  Park_It[1][3] = 1.0;\n  Park_It[1][4] = 2.0;\n  Park_It[2][0] = 0.0;\n  Park_It[2][1] = 1.0;\n  Park_It[2][2] = 1.0;\n  Park_It[2][3] = 2.0;\n  Park_It[2][4] = 1.0;\n  Park_It[3][0] = 0.0;\n  Park_It[3][1] = 0.0;\n  Park_It[3][2] = 0.0;\n  Park_It[3][3] = 0.0;\n  Park_It[3][4] = 0.0;\n  Park_It[4][0] = 0.0;\n  Park_It[4][1] = 1.0;\n  Park_It[4][2] = 2.0;\n  Park_It[4][3] = 2.0;\n  Park_It[4][4] = 1.0;while ((Row < 6.0)) {\n    Col = 1.0;\n    while ((Col < 6.0)) {\n      if (Park_It[static_cast<int>(Row) - 1][static_cast<int>(Col) - 1] == 1.0) {\n        Brain.Screen.setPenColor(red);\n        Brain.Screen.print(\"*\");\n      } else if (Park_It[static_cast<int>(Row) - 1][static_cast<int>(Col) - 1] == 2.0) {\n        Brain.Screen.setPenColor(green);\n        Brain.Screen.print(\".\");\n      } else {\n        Brain.Screen.setPenColor(blue);\n        Brain.Screen.print(\"!\");\n      }\n      Col = Col + 1.0;\n    wait(5, msec);\n    }\n    Brain.Screen.newLine();\n    Row = Row + 1.0;\n  wait(5, msec);\n  }\n  return 0;\n}\n\n\nint main() {\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // Wait for ADI sensors to initialize\n  wait(100, msec);\n\n\n  whenStarted1();\n}","target":"Physical"}